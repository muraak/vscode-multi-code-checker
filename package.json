{
    "name": "multi-code-checker",
    "displayName": "Multi Code Checker",
    "description": "Check your code by your compiler.",
    "version": "0.0.1",
    "publisher": "muraak",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.check",
        "onLanguage:c"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "mcc.check",
                "title": "Check Code"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
